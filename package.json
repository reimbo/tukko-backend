{
  "name": "traffic-visualizer-backend",
  "version": "1.0.0",
  "description": "Backend for the Traffic Visualizer project. Acts as a inbetween to traffic data APIs and the enduser, to reduce load on the traffic APIs",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc -p .",
    "serve": "node dist/index.js",
    "test": "jest",
    "dev": "npx tsc -p . && docker compose -f dev-compose.yml up -d --build && node ./dist/index.js",
    "prod": "npx tsc -p . && docker compose -f docker-compose.yml up -d --build"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.labranet.jamk.fi/wimma-lab-2023/iotitude/traffic-visualizer-backend"
  },
  "author": "IoTitude",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.4.0",
    "express": "^4.18.2",
    "http-status-codes": "^2.2.0",
    "mongodb": "^5.6.0",
    "node-localstorage": "^2.2.1",
    "redis": "^4.6.7",
    "redis-om": "^0.4.0-beta.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.2.5",
    "@types/node-localstorage": "^1.3.0",
    "@types/react": "^18.2.12",
    "dotenv": "^16.1.4",
    "jest": "^29.5.0",
    "nodemon": "^1.14.9",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.3"
  }
}
